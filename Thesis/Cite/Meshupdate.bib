file = {:Users/Andreas/Google Drive/Master/FSI/Meshupdate/JasakTukovic{\_}MeshMotion{\_}FAMENA{\_}26-11-2013.pdf:pdf},
isbn = {1333-1124},
issn = {13331124},
journal = {Transactions of Famena},
keywords = {Arbitrarily unstructured mesh,Finite volume,Free-surface,Motion solver,Moving mesh,Polyhedral mesh,Surface tracking,Vertex motion},
number = {2},
pages = {1--20},
title = {{Automatic mesh motion for the unstructured Finite Volume Method}},
