journal = {New Methods in Transient Analysis ASME},
number = {143},
title = {{COMPUTATION OF UNSTEADY INCOMPRESSIBLE FLOWS WITH THE STABILIZED FINITE ELEMENT METHODS: SPACE-TIME FORMULATIONS, ITERATIVE STRATEGIES AND MASSIVELY PARALLEL IMPLEMENTATIONSt}},
url = {https://pdfs.semanticscholar.org/4290/ba11e1f4499481ad683bba73d85d4eec6fe5.pdf},
